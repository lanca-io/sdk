			"import": "./dist/index.es.js"
		},
		"./abi": "./dist/abi/index.js",
		"./configs": "./dist/configs/index.js",
		"./constants": "./dist/constants/index.js",
		"./core": "./dist/core/index.js",
		"./errors": "./dist/errors/index.js",
		"./types": "./dist/types/index.js",
		"./utils": "./dist/utils/index.js"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"url": "git://github.com/concero/sdk.git"
	},
	"scripts": {
		"build": "npx vite build && tsc",
		"test": "jest",
		"format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
		"lint": "eslint \"src/**/*.{ts,js}\"",
		"publish": "npm publish"
	},
	"devDependencies": {
		"@eslint/js": "9.11.1",
		"@jest/globals": "29.7.0",
		"@types/jest": "29.5.14",
		"@types/node": "20.12.11",
		"@typescript-eslint/eslint-plugin": "8.7.0",
		"@typescript-eslint/parser": "8.7.0",
		"eslint": "9.11.1",
		"globals": "15.9.0",
		"jest": "29.7.0",
		"pino-pretty": "12.1.0",
		"prettier": "3.3.3",
		"swc": "1.0.11",
		"ts-jest": "29.2.5",
		"ts-node": "10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "5.7.2",
		"typescript-eslint": "^8.7.0",
		"vite": "6.0.3"
	},
	"dependencies": {
		"pino": "9.5.0",
		"solady": "0.0.281",
		"viem": "2.10.3"
	}
