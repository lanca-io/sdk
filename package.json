{
	"name": "lanca-sdk-demo",
	"version": "0.0.11",
	"description": "Lanca.io SDK built with Typescript",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.es.js",
	"types": "./dist/index.d.ts",
	"author": "Lanca.io",
	"license": "ISC",
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"require": "./dist/index.cjs.js",
			"import": "./dist/index.es.js"
		}
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"url": "git://github.com/concero/sdk.git"
	},
	"scripts": {
		"build": "npx vite build && tsc --project ./tsconfig.json && tsc --project ./tsconfig.cjs.json",
		"test": "vitest",
		"format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
		"lint": "eslint \"src/**/*.{ts,js}\"",
		"publish": "npm publish"
	},
	"devDependencies": {
		"@eslint/js": "9.11.1",
		"@types/jest": "^29.5.14",
		"@types/node": "20.12.11",
		"@typescript-eslint/eslint-plugin": "8.7.0",
		"@typescript-eslint/parser": "8.7.0",
		"eslint": "9.11.1",
		"globals": "15.9.0",
		"prettier": "3.3.3",
		"ts-node": "10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "5.7.2",
		"typescript-eslint": "^8.7.0",
		"vite": "^6.0.5",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"pino": "9.5.0",
		"pino-pretty": "12.1.0",
		"solady": "0.0.286",
		"viem": "2.21.57"
	},
	"keywords": [
		"sdk",
		"ethereum",
		"dapp",
		"bridge",
		"swap",
		"web3",
		"viem",
		"cross-chain",
		"defi",
		"cross-chain-applications",
		"cross-chain-bridge",
		"multi-chain",
		"metamask",
		"lanca",
		"dex",
		"concero"
	]
}
