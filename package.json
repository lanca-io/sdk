{
	"name": "lanca-sdk-demo",
	"version": "0.0.60",
	"description": "Lanca.io SDK built with Typescript",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.es.js",
	"types": "./dist/index.d.ts",
	"author": "Lanca.io",
	"license": "ISC",
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"require": "./dist/index.cjs.js",
			"import": "./dist/index.es.js"
		}
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"url": "git://github.com/lanca-io/sdk.git"
	},
	"scripts": {
		"build": "npx vite build && tsc --project ./tsconfig.json && tsc --project ./tsconfig.cjs.json",
		"test": "vitest",
		"format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
		"lint": "npx eslint . --config eslint.config.mjs",
		"lint:fix": "npx eslint . --config eslint.config.mjs --fix",
		"publish": "npm publish",
		"prepare": "husky"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run format && npm run lint:fix && npm run build"
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.5",
		"@typescript-eslint/eslint-plugin": "^8.19.1",
		"@typescript-eslint/parser": "^8.19.1",
		"eslint": "^9.17.0",
		"globals": "15.9.0",
		"husky": "^9.1.7",
		"prettier": "^3.4.2",
		"ts-node": "10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.19.0",
		"vite": "^6.0.7",
		"vitest": "^2.1.8",
		"@rollup/rollup-darwin-arm64": "4.32.0"
	},
	"dependencies": {
		"pino": "9.6.0",
		"pino-pretty": "13.0.0",
		"solady": "0.0.288",
		"viem": "2.22.3"
	},
	"keywords": [
		"sdk",
		"ethereum",
		"dapp",
		"bridge",
		"swap",
		"web3",
		"viem",
		"cross-chain",
		"defi",
		"cross-chain-applications",
		"cross-chain-bridge",
		"multi-chain",
		"metamask",
		"lanca",
		"dex",
		"concero"
	]
}
