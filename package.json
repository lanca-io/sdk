{
	"name": "lanca-sdk-demo",
	"version": "0.0.4",
	"description": "Lanca.io SDK built with Typescript",
	"main": "./dist/index.js",
	"module": "./dist/index.es.js",
	"types": "./dist/typings/index.d.ts",
	"author": "",
	"license": "ISC",
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
